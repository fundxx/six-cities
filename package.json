{
  "name": "six-cities",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://fundxx.github.io/six-cities",
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server",
    "eslint": "eslint --ext .jsx --ext .js src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "deploy": "gh-pages -d public"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/style-mock.js",
      "^Project(.*)$": "<rootDir>/src$1"
    },
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^4.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "history": "^4.10.1",
    "jest": "^26.6.3",
    "leaflet": "^1.7.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^8.3.2"
  }
}
